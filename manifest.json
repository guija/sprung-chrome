{
  "manifest_version": 2,

  "name": "Sprung for Chrome",
  "description": "Sprung for Chrome",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": { 
    "page": "background.html"
  },

  "permissions": [
    "background",
    "activeTab",
    "tabs",
    "http://localhost:1234/"
  ]
}